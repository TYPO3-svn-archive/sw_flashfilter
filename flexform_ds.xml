<?xml version="1.0" encoding="utf-8" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets type="array">
		<sDEF type="array">
			<ROOT type="array">
				<TCEforms type="array">
					<sheetTitle>Flash Container</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<source>
						<type>array</type>
						<el>
							<flash_width>
								<TCEforms>
									<label>Flash Width</label>
									<config>
										<type>input</type>
										<size>3</size>
									</config>
								</TCEforms>
							</flash_width>
							<flash_height>
								<TCEforms>
									<label>Flash Height</label>
									<config>
										<type>input</type>
										<size>3</size>
									</config>
								</TCEforms>
							</flash_height>
							<flash_source>
								<TCEforms>
									<label>Flash Source</label>
									<config>
										<type>group</type>
										<internal_type>file</internal_type>
										<allowed>swf</allowed>
										<max_size>25000000</max_size>
										<disallowed_types>php,php3</disallowed_types>
										<maxitems>1</maxitems>
										<minitems>0</minitems>
										<show_thumbs>1</show_thumbs>
										<autoSizeMax>1</autoSizeMax>
									</config>
								</TCEforms>
							</flash_source>
							<flash_fallback>
								<TCEforms>
									<label>Flash Fallback Image</label>
									<config>
										<type>group</type>
										<maxitems>1</maxitems>
										<minitems>0</minitems>
										<internal_type>file</internal_type>
										<allowed>jpg,jpeg,png,gif</allowed>
										<autoSizeMax>1</autoSizeMax>
										<show_thumbs>1</show_thumbs>
									</config>
								</TCEforms>
							</flash_fallback>
						</el>
					</source>
				</el>
			</ROOT>
		</sDEF>
	</sheets>

</T3DataStructure>